{
  "name": "meridiano-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "briefing": "ts-node src/scripts/runBriefing.ts",
    "briefing:tech": "ts-node src/scripts/runBriefing.ts --feed technology",
    "briefing:brasil": "ts-node src/scripts/runBriefing.ts --feed brasil",
    "briefing:teclas": "ts-node src/scripts/runBriefing.ts --feed teclas",
    "briefing:scrape": "ts-node src/scripts/runBriefing.ts --scrape",
    "briefing:process": "ts-node src/scripts/runBriefing.ts --process",
    "briefing:rate": "ts-node src/scripts/runBriefing.ts --rate",
    "briefing:generate": "ts-node src/scripts/runBriefing.ts --generate"
  },
  "dependencies": {
    "@mozilla/readability": "^0.5.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "axios": "^1.7.7",
    "cheerio": "^1.0.0",
    "commander": "^12.1.0",
    "dotenv": "^17.2.3",
    "jsdom": "^23.0.0",
    "marked": "^17.0.1",
    "ml-kmeans": "^6.0.0",
    "moment": "^2.30.1",
    "openai": "^4.67.3",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.2",
    "rss-parser": "^3.13.0",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/cheerio": "^0.22.35",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.0",
    "@types/marked": "^6.0.0",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.10.9",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
